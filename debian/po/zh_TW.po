#. Type: select
#: ../templates:3
msgid "www.dyndns.org"
msgstr "www.dyndns.org"

#. Type: select
#: ../templates:3
msgid "www.easydns.com"
msgstr "www.easydns.com"

#. Type: select
#: ../templates:3
msgid "www.dslreports.com"
msgstr "www.dslreports.com"

#. Type: select
